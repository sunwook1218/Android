<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/gnb_gsty_group_container"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/gnb_group_title_wrap"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        android:paddingLeft="13.3dp"
        android:paddingRight="13.3dp"
        android:background="#fff"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/gnb_group_title_text"
            android:layout_width="48.3dp"
            android:layout_height="wrap_content"
            android:text="결재"
            android:minHeight="18.3dp"
            android:textSize="14.7sp"
            android:textColor="#4b4b4b"
            android:singleLine="true"
            android:paddingTop="15dp"
            android:background="#fff"
             />

        <com.hs.mobile.gw.util.GridViewEx
            android:id="@+id/m_show_list"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:listSelector="@drawable/lnb_menu_gd_selector"
            android:background="#fff"
            android:numColumns="4"
            />

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0.3dp"
        android:background="#bdbdbd"
        android:layout_marginLeft="13.3dp"
        android:layout_marginRight="13.3dp">
    </LinearLayout>

</LinearLayout>