package com.hs.mobile.gw.util;

public class CommonController<V, M> {
	private V view;
	private M model;

	public CommonController(V view, M model) {
		this.view = view;
		this.model = model;
	}

	public V getView() {
		return this.view;
	}

	public M getModel() {
		return this.model;
	}
}
