<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical" >

    <include layout="@layout/fragment_sub_header"></include>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="71dp"
        android:paddingLeft="13.3dp"
        android:paddingRight="13.3dp"
        android:background="@drawable/border_bottom">

        <ImageView
            android:id="@+id/ID_BTN_BACK"
            android:layout_width="39dp"
            android:layout_height="46.17dp"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:padding="15dp"
            android:contentDescription="@string/dummy_string"
            android:src="@drawable/style_btn_back" />

        <ImageView
            android:id="@+id/ID_BTN_MENU"
            android:layout_width="17.3dp"
            android:layout_height="14dp"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:layout_toRightOf="@id/ID_BTN_BACK"
            android:contentDescription="@string/dummy_string"
            android:src="@drawable/style_btn_menu"
            android:visibility="gone" />

        <TextView
            android:id="@+id/ID_TV_NAVE_TITLE"
            style="@style/header_title_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerInParent="true"
            android:ellipsize="end"
            android:gravity="center"
            android:maxLines="2"
            android:paddingLeft="45dp"
            android:paddingRight="45dp"
            android:text="@string/label_square_group_info"
            android:textSize="18sp"
            android:textStyle="bold"
            android:typeface="sans" />
    </RelativeLayout>

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="3dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:text="@string/label_square_group_info"
        android:textColor="#495057"
        android:textSize="12sp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="#f1f1f1" >
    </LinearLayout>

    <LinearLayout
        android:id="@+id/ID_LAY_L_GROUP_TITLE"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:clickable="true" >

        <TextView
            android:id="@+id/ID_TV_GROUP_TITLE"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:textColor="@color/black"
            android:textSize="15sp" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#efefef" >
    </View>

    <LinearLayout
        android:id="@+id/ID_LAY_L_FAVORITE_GROUP"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <LinearLayout
            android:id="@+id/ID_LAY_L_SETTING_FAVORITE"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:padding="10dp" >

            <TextView
                android:id="@+id/TextView01"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/label_square_group_favorite"
                android:textColor="@color/black"
                android:textSize="15sp" />

            <CheckBox
                android:id="@+id/ID_CB_FAVORITE_GROUP"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:button="@drawable/style_btn_checkbox_blue" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#efefef" >
        </View>
    </LinearLayout>
<LinearLayout
        android:id="@+id/ID_LAY_L_NOTIFY_SETTING"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="3dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="30dp"
            android:text="@string/label_notify_setting_string"
            android:textColor="#495057"
            android:textSize="12sp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="#f1f1f1" >
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:clickable="true"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="10dp" >

            <TextView
                android:id="@+id/ID_TV_NOTIFY_SETTING"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/label_notify_all"
                android:textColor="@color/black"
                android:textSize="15sp" />

<!--             <TextView -->
<!--                 android:layout_width="wrap_content" -->
<!--                 android:layout_height="wrap_content" -->
<!--                 android:layout_marginRight="10dp" -->
<!--                 android:text="&gt;" -->
<!--                 android:textColor="#999999" -->
<!--                 android:textSize="12sp" /> -->
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#efefef" >
        </View>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/ID_LAY_L_GROUP_MEMBER"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/TextView02"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="3dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="30dp"
            android:text="@string/label_square_group_participant_info"
            android:textColor="#495057"
            android:textSize="12sp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="#f1f1f1" >
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ID_LAY_L_SETTING_PARTICIPANT"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:clickable="true"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="10dp" >

            <TextView
                android:id="@+id/TextView03"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/label_square_new_work_group_participant"
                android:textColor="@color/black"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/ID_TV_SETTING_PARTICIPANT_COUNT"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="10dp"
                android:text="10"
                android:textColor="#999999"
                android:textSize="12sp" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#efefef" >
        </View>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/ID_LAY_L_GROUP_ACTIVITY"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/TextView04"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="3dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="30dp"
            android:text="@string/label_square_setting_change_activity"
            android:textColor="#495057"
            android:textSize="12sp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="#f1f1f1" >
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ID_LAY_L_END_ACTIVITY"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="true"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/ID_TV_ACTIVITY_LABEL"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="10dp"
                android:text="@string/label_square_setting_end_activity_label"
                android:textColor="@color/black"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/ID_TV_ACTIVITY_DESC"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="3dp"
                android:text="@string/label_square_setting_end_activity"
                android:textColor="#999999"
                android:textSize="12sp" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#efefef" >
        </View>
    </LinearLayout>

</LinearLayout>