/***** 8.0 bbs *****/
.basic_table {
	width:100%;
	border: 1px solid #d5d5d9;
	border-top: 0;
	border-right: 0
}
.basic_table th, .basic_table td {
	height: 22px;
	padding: 3px 5px;
	border-top: 1px solid #d5d5d5;
	border-right: 1px solid #d5d5d5;
	line-height: 22px
}
.basic_table th {
    width:15% !important;
	background-color: #f5f5f5;
	font-weight: normal;
	text-align: center;
	color: #666
}

.basic_table td {
    width:35% !important;
}

#bbsinfoDiv{
    width:100%;
    position: relative;
    margin: 15px 0 105px 0;
}

.pivot_table{
    font-size: 9pt; width:100%;
    display:block;
    text-align: center;
}
.pivot_table tbody, .pivot_table tbody tr {display:block;}
.pivot_table th{
    background-color: #9dabc1;
    color: white;
    display: block;
    box-sizing: border-box;
    position: absolute;
    top: 0;
    line-height: 23.3pt;
    vertical-align: middle;
    font-weight: normal;
}
.pivot_table td {
    display: block;
    box-sizing: border-box;
    position: absolute;
    top: 23.3pt;
    vertical-align: middle;
    background-color: #f5f5f5;
    color: #4a4a4a;
    line-height: 30pt;
}
.pivot_table th:last-of-type {width:25%;}
.pivot_table td:last-of-type {width:25%;}
.pivot_table th:nth-of-type(1) {left:0; width: 15%;}
.pivot_table th:nth-of-type(2) {left:15%; width: 15%;}
.pivot_table th:nth-of-type(3) {left:30%; width: 15%;}
.pivot_table th:nth-of-type(4) {left:45%; width: 15%;}
.pivot_table th:nth-of-type(5) {left:60%; width: 15%;}
.pivot_table th:last-of-type {left:75%; width: 25%;}
.pivot_table td:nth-of-type(1) {left:0; width: 15%;}
.pivot_table td:nth-of-type(2) {left:15%; width: 15%;}
.pivot_table td:nth-of-type(3) {left:30%; width: 15%;}
.pivot_table td:nth-of-type(4) {left:45%; width: 15%;}
.pivot_table td:nth-of-type(5) {left:60%; width: 15%;}
.pivot_table td:last-of-type {left:75%; width: 25%;}

